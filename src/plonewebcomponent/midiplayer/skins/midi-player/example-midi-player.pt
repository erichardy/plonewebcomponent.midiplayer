<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en"
      xmlns:tal="http://xml.zope.org/namespaces/tal"
      xmlns:metal="http://xml.zope.org/namespaces/metal"
      xmlns:i18n="http://xml.zope.org/namespaces/i18n"
      lang="en"
      metal:use-macro="context/main_template/macros/master"
      i18n:domain="plone">

<body>

    <metal:content-core fill-slot="content-core">
    <span id="SoundFontBase" tal:content="string:${context/portal_url}">sound font base</span>
    <link rel="import" href="++plone++midiplayer/midi-player.html">
    <span tal:define="base string:${context/portal_url}">
      <midi-player tal:attributes="SoundFontBase string:${base}" url="++plone++midiplayer/ColmanCross1.mid"></midi-player>
    </span>
    </metal:content-core>

</body>
</html>
